{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LogConnection": "Server=sql5,1433;Database=db-ms-loggers;User Id=sa;Password=Your_password123;Encrypt=True;TrustServerCertificate=True;"
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "jwt-autentica-bearer",
    "Audience": "jwt-autentica-bearer"
  },
  "ReverseProxy": {
    "Routes": {
      "catalogRoute": {
        "ClusterId": "catalogCluster",
        "Match": {
          "Path": "/catalog/{**catch-all}"
        }
      },
      "ordersRoute": {
        "ClusterId": "ordersCluster",
        "Match": {
          "Path": "/orders/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "catalogCluster": {
        "Destinations": {
          "catalogApi": {
            "Address": "http://catalogservice:8080"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      },
      "ordersCluster": {
        "Destinations": {
          "ordersApi": {
            "Address": "https://orderservice:8081"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      }
    }
  }
}
